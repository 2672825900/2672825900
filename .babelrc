{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "scss$"
        ]
      },
      ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
    ],
    "transform-class-properties",
    "transform-decorators-legacy"
  ]
}
